    "rollup-plugin-dev": "^2.0.4",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-scss": "^4.0.0",
    "rollup-plugin-serve": "^1.1.1",
    "sass": "^1.75.0",
    "serve": "^14.2.1",
    "tslib": "^2.6.2",
    "typescript": "^5.4.5",
    "uglify-js": "^3.17.4"
